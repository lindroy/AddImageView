<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="AddImageView">
        <attr name="maxCount" format="integer" />
        <attr name="spanCount" format="integer" />
        <attr name="itemGap" format="dimension|reference" />

        <attr name="enableDrag" format="boolean" />
        <attr name="enableVibrate" format="boolean" />
        <attr name="vibrateDuration" format="integer" />

        <attr name="enableAnimation" format="boolean" />
        <attr name="animDuration" format="integer" />
        <attr name="scaleValue" format="float" />
    </declare-styleable>
</resources>